,QAAA,KAAA,EAAA,KAAA,YAAA,CAAA,KAJA;AAKA,QAAA,KAAA,EAAA,KAAA,YAAA,CAAA,KALA;AAMA,QAAA,SAAA,EAAA;AANA,OAAA;AASA,MAAA,gBAAA,CAAA,MAAA,CAAA,KAAA,YAAA,CAAA,EAAA,EAAA,IAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,YAAA,CAAA,SAAA,GAAA,MAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OAPA;AAQA,KA9BA;AAgCA,IAAA,WAhCA,yBAgCA;AAAA;;AACA,MAAA,gBAAA,CAAA,MAAA,CAAA,KAAA,YAAA,CAAA,EAAA,EAAA,KAAA,YAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,qCAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OAPA;AAQA,KAzCA;AA2CA,IAAA,WA3CA,yBA2CA;AAAA;;AACA,MAAA,gBAAA,CAAA,MAAA,CAAA,KAAA,YAAA,CAAA,EAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OAPA;AAQA;AApDA,GARA;AA8DA,EAAA,OA9DA,qBA8DA;AACA,SAAA,OAAA,GAAA,EAAA;AACA,SAAA,QAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA;AAjEA,CAAA","sourcesContent":["<template>\n  <div v-if=\"currentGuide\" class=\"edit-form\">\n    <h4>User</h4>\n    <form>\n\n      <div class=\"form-group\">\n        <label for=\"title\">Title</label>\n        <input type=\"text\" class=\"form-control\" id=\"title\"\n          v-model=\"currentGuide.title\"\n        />\n\n      </div>\n      <div class=\"form-group\">\n        <label for=\"password\">Password</label>\n        <input type=\"text\" class=\"form-control\" id=\"password\"\n          v-model=\"currentGuide.password\"\n        />\n\n      </div>\n      <div class=\"form-group\">\n        <label for=\"phone\">Phone</label>\n        <input type=\"text\" class=\"form-control\" id=\"phone\"\n          v-model=\"currentGuide.phone\"\n        />\n\n      </div>\n      <div class=\"form-group\">\n        <label for=\"email\">Email</label>\n        <input type=\"text\" class=\"form-control\" id=\"email\"\n          v-model=\"currentGuide.email\"\n        />\n      </div>\n      <div class=\"form-group\">\n        <label><strong>Status:</strong></label>\n        {{ currentGuide.published ? \"Published\" : \"Pending\" }}\n      </div>\n      \n    </form>\n\n    <button class=\"badge badge-primary mr-2\"\n      v-if=\"currentGuide.published\"\n      @click=\"updatePublished(false)\"\n    >\n      UnPublish\n    </button>\n    <button v-else class=\"badge badge-primary mr-2\"\n      @click=\"updatePublished(true)\"\n    >\n      Publish\n    </button>\n\n    <button class=\"badge badge-danger mr-2\"\n      @click=\"deleteGuide\"\n    >\n      Delete\n    </button>\n\n    <button type=\"submit\" class=\"badge badge-success\"\n      @click=\"updateGuide\"\n    >\n      Update\n    </button>\n    <p>{{ message }}</p>\n  </div>\n\n  <div v-else>\n    <br />\n    <p>Please click on a Guide...</p>\n  </div>\n</template>\n\n<script>\nimport UserDataService from \"../services/UserDataService\";\n\nexport default {\n  name: \"user\",\n  data() {\n    return {\n      currentUser: null,\n      message: ''\n    };\n  },\n  methods: {\n    getUser(id) {\n      UserDataService.get(id)\n        .then(response => {\n          this.currentUser = response.data;\n          console.log(response.data);\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n\n    updatePublished(status) {\n      var data = {\n        id: this.currentUser.id,\n        title: this.currentUser.title,\n        password: this.currentUser.password,\n        phone: this.currentGuide.phone,\n        email: this.currentGuide.email,\n        published: status\n      };\n\n      GuideDataService.update(this.currentGuide.id, data)\n        .then(response => {\n          this.currentGuide.published = status;\n          console.log(response.data);\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n\n    updateGuide() {\n      GuideDataService.update(this.currentGuide.id, this.currentGuide)\n        .then(response => {\n          console.log(response.data);\n          this.message = 'The Guide was updated successfully!';\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n\n    deleteGuide() {\n      GuideDataService.delete(this.currentGuide.id)\n        .then(response => {\n          console.log(response.data);\n          this.$router.push({ name: \"Guides\" });\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    }\n  },\n  mounted() {\n    this.message = '';\n    this.getGuide(this.$route.params.id);\n  }\n};\n</script>\n\n<style>\n.edit-form {\n  max-width: 300px;\n  margin: auto;\n}\n</style>\n"],"sourceRoot":"src/components"}]}