ACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,OALA,EAMA,KANA,CAMA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OARA;AASA,KAnBA;AAqBA,IAAA,QArBA,sBAqBA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA;AAxBA;AAbA,CAAA","sourcesContent":["<template>\n  <div class=\"submit-form\">\n    <div v-if=\"!submitted\">\n      \n      <div class=\"form-group\">\n        <label for=\"title\">Name</label>\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"title\"\n          required\n          v-model=\"guide.title\"\n          name=\"title\"\n        />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"password\">Password</label>\n        <input\n          class=\"form-control\"\n          id=\"password\"\n          required\n          v-model=\"guide.password\"\n          name=\"password\"\n        />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"description\">Description</label>\n        <input\n          class=\"form-control\"\n          id=\"description\"\n          required\n          v-model=\"guide.description\"\n          name=\"description\"\n        />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"phone\">Phone</label>\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"phone\"\n          required\n          v-model=\"guide.phone\"\n          name=\"phone\"\n        />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"email\">Email</label>\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"email\"\n          required\n          v-model=\"guide.email\"\n          name=\"email\"\n        />\n      </div>\n\n      <button @click=\"saveGuide\" class=\"btn btn-success\">Submit</button>\n    </div>\n\n    <div v-else>\n      <h4>You submitted successfully!</h4>\n      <button class=\"btn btn-success\" @click=\"newGuide\">Add</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport GuideDataService from \"../services/GuideDataService\";\n\nexport default {\n  name: \"add-guide\",\n  data() {\n    return {\n      guide: {\n        id: null,\n        title: \"\",\n        description: \"\",\n        published: false\n      },\n      submitted: false\n    };\n  },\n  methods: {\n    saveGuide() {\n      var data = {\n        title: this.guide.title,\n        password: this.guide.password,\n        description: this.guide.description,\n        phone: this.guide.phone,\n        email: this.guide.email,\n      };\n\n      GuideDataService.create(data)\n        .then(response => {\n          this.guide.id = response.data.id;\n          console.log(response.data);\n          this.submitted = true;\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n\n    newGuide() {\n      this.submitted = false;\n      this.guide = {};\n    }\n  }\n};\n</script>\n\n<style>\n.submit-form {\n  max-width: 300px;\n  margin: auto;\n}\n</style>\n"],"sourceRoot":"src/components"}]}